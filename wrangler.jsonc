{
  "name": "insidemordecai",
  "compatibility_date": "2026-02-19",
  "assets": {
    "directory": "./public",
    "html_handling": "auto-trailing-slash",
    "not_found_handling": "404-page",
    "run_worker_first": false
  },
  "build": {
    "command": "hugo build --gc --minify",
  },
  "[vars]": {  // Environment variables passed to build
    "HUGO_VERSION": "extended_0.155.3",
    "HUGO_ENV": "production"
  },
  "workers_dev": true,
  "preview_urls": true
}
